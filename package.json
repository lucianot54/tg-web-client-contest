    "postcss": "^7.0.21",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-minify-html-literals": "^1.2.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^5.1.2",
    "serve": "^11.2.0"
